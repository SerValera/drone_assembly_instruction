<launch>
	<!-- vim: set ft=xml noet : -->
	<!-- example launch script for ArduPilot based FCU's -->
		<arg name="fcu_url" default="/dev/ttyS4:115200" />
		<!-- <arg name="fcu_url" default="/dev/ttyACM0:115200" /> -->
		
		<arg name="gcs_url" default="" />
		<arg name="tgt_system" default="1" />
		<arg name="tgt_component" default="1" />
		<arg name="log_output" default="screen" />
		<arg name="fcu_protocol" default="v2.0" />
		<arg name="respawn_mavros" default="false" />

		<include file="$(find mavros)/launch/node.launch">
			<arg name="pluginlists_yaml" value="$(find mavros)/launch/apm_pluginlists.yaml" />
			<arg name="config_yaml" value="$(find mavros)/launch/apm_config.yaml" />

			<arg name="fcu_url" value="$(arg fcu_url)" />
			<arg name="gcs_url" value="$(arg gcs_url)" />
			<arg name="tgt_system" value="$(arg tgt_system)" />
			<arg name="tgt_component" value="$(arg tgt_component)" />
			<arg name="log_output" value="$(arg log_output)" />
			<arg name="fcu_protocol" value="$(arg fcu_protocol)" />
			<arg name="respawn_mavros" value="$(arg respawn_mavros)" />
		</include>


    <include file="$(find vicon_bridge)/launch/vicon.launch">
		<arg name="datastream_hostport" value="192.168.0.170"/>
	</include>

    <node name="converter2mav" pkg="racing_pkg" type="vicon2mavros.py" respawn="true" />
	<!-- <include file="$(find fkie_master_discovery)/launch/master_discovery.launch"/> -->

    <node name="origin_setter" pkg="vision_to_mavros" type="set_origin.py" respawn="false" />
    <!-- <node name="follower" pkg="racing_pkg" type="follower.py" respawn="true" /> -->

    <!-- Launch Real Seance D435 node -->
    <group ns="d435">
        <include file="$(find realsense2_camera)/launch/rs_camera.launch">
			<arg name="filters" value="pointcloud,decimation,spatial"/>
			<arg name="pointcloud_texture_stream" value="RS2_STREAM_ANY"/>
			<arg name="initial_reset" value="true"/>
			<arg name="color_fps" value="30"/>
			<arg name="depth_fps" value="30"/>
        </include>
    </group>

    <!-- Launch Real Seance T265 node -->
    <group ns="t265">
        <include file="$(find realsense2_camera)/launch/rs_t265.launch">
			<arg name="initial_reset" value="true"/>
            <arg name="enable_fisheye1" value="true"/>
            <arg name="enable_fisheye2" value="true"/>
        </include>
    </group>


</launch>
