<launch>

    <include file="$(find vicon_bridge)/launch/vicon.launch">
		<arg name="datastream_hostport" value="192.168.0.170"/>
	</include>

    <include file="$(find realsense2_camera)/launch/rs_camera.launch">
		<arg name="enable_depth" value="false"/>
		<arg name="color_width" value = "1920"/>
		<arg name="color_height" value = "1080"/>
		<arg name="color_fps" value = "8"/>
	</include>

    <node name="converter2cam" pkg="racing_pkg" type="map2cam_broadcaster.py" respawn="true" />
    <node name="dataset_collector" pkg="racing_pkg" type="subscr_ros1.py" respawn="true" />



</launch>